{
  "name": "@glimmer/tracking",
  "version": "2.0.0",
  "description": "Glimmer tracking library",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "ember-addon"
  ],
  "exports": {
    ".": "./index.ts",
    "./primitives/cache": "./primitives/cache.ts"
  },
  "publishConfig": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./primitives/cache": {
      "types": "./dist/primitives/cache.d.ts",
      "default": "./dist/primitives/cache.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.9",
    "@glimmer/env": "^0.1.7",
    "@ember/-internals": "workspace:*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs"
  }
}

